{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "build:start": "npm run build -- --profile=pathway && npm run start",
    "start": "sam local start-api -t cdk.out/SyllexApiV2.template.json --profile=pathway",
    "fix:pdfparse": "rm node_modules/pdf-parse/index.js && cp index.js node_modules/pdf-parse/index.js",
    "deploy": "npm run fix:pdfparse && npm run bootstrap && npm run build && cdk deploy",
    "deploy:local": "npm run fix:pdfparse && cdk bootstrap --profile=pathway && cdk synth && cdk deploy --profile=pathway",
    "create:user": "npx ts-node routines/createUser.ts",
    "dev": "AWS_PROFILE=pathway nodemon --exec tsx ./local/local-dev.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@colors/colors": "^1.6.0",
    "@types/aws-lambda": "^8.10.146",
    "@types/bcrypt": "^6.0.0",
    "@types/busboy": "^1.5.4",
    "@types/express": "^5.0.6",
    "@types/http-errors": "^2.0.5",
    "@types/ini": "^4.1.1",
    "@types/jest": "^29.5.14",
    "@types/jszip": "^3.4.0",
    "@types/node": "22.7.9",
    "@types/pdf-parse": "^1.1.5",
    "@types/xml2js": "^0.4.14",
    "aws-cdk": "2.174.1",
    "dotenv": "^16.5.0",
    "esbuild": "^0.24.0",
    "ini": "^5.0.0",
    "inquirer": "^12.6.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-cdk/aws-amplify-alpha": "^2.174.1-alpha.0",
    "@aws-sdk/client-bedrock-runtime": "^3.864.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.830.0",
    "@aws-sdk/client-lambda": "^3.865.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/client-secrets-manager": "^3.835.0",
    "@aws-sdk/client-ses": "^3.920.0",

{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "build:yaml": "cdk synth > cdk.out/template.yaml",
    "start": "npm run build:yaml -- --profile=pathway && sam local start-api --template cdk.out/template.yaml --profile=pathway",
    "fix:pdfparse": "rm node_modules/pdf-parse/index.js && cp index.js node_modules/pdf-parse/index.js",
    "deploy": "npm run fix:pdfparse && npm run bootstrap && npm run build && cdk deploy",
    "deploy:local": "npm run fix:pdfparse && cdk bootstrap --profile=pathway && cdk synth && cdk deploy --profile=pathway",
    "create:function": "npx ts-node routines/createFunction.ts",
    "create:user": "npx ts-node routines/createUser.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@colors/colors": "^1.6.0",
    "@types/aws-lambda": "^8.10.146",
    "@types/bcrypt": "^6.0.0",
    "@types/busboy": "^1.5.4",
    "@types/ini": "^4.1.1",
    "@types/jest": "^29.5.14",
    "@types/jszip": "^3.4.0",
    "@types/markdown-pdf": "^9.0.5",
    "@types/node": "22.7.9",
    "@types/pdf-parse": "^1.1.5",
    "@types/xml2js": "^0.4.14",
    "aws-cdk": "2.174.1",
    "dotenv": "^16.5.0",
    "esbuild": "^0.24.0",
    "ini": "^5.0.0",
    "inquirer": "^12.6.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-cdk/aws-amplify-alpha": "^2.174.1-alpha.0",
    "@aws-sdk/client-bedrock-runtime": "^3.864.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.830.0",
    "@aws-sdk/client-lambda": "^3.865.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/client-secrets-manager": "^3.835.0",
    "@aws-sdk/client-ses": "^3.920.0",
    "@aws-sdk/client-sqs": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@dqbd/tiktoken": "^1.0.22",
    "@google-cloud/documentai": "^9.4.0",
    "@mermaid-js/mermaid-cli": "^11.9.0",
    "@types/bcryptjs": "^2.4.6",
    "aws-cdk-lib": "2.174.1",
    "bcryptjs": "^3.0.2",
    "busboy": "^1.6.0",
    "constructs": "^10.0.0",
    "js-tiktoken": "^1.0.21",
    "jszip": "^3.10.1",
    "mammoth": "^1.10.0",
    "markdown-pdf": "^11.0.0",
    "mermaid": "^10.0.0",
    "mongodb": "^6.17.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pptxgenjs": "^4.0.1",
    "puppeteer-core": "^24.18.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  }
}

{
  "name": "syllex-def",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "spec:export": "aws apigateway get-export --rest-api-id bkwshpcns1 --stage-name prod --export-type oas30 --accepts application/yaml --parameters extensions='integrations' openapi.yaml --region eu-south-1 --profile=pathway",
    "generate:api": "openapi-generator-cli generate -i ./openapi.yaml -g typescript-angular -o ./src/app/api --skip-validate-spec --additional-properties ngVersion=20,useSingleRequestParameter=true",
    "update:api": "npm run spec:export; npm run generate:api"
  },
  "private": true,
